package com.eme.ims.codec;import java.io.Serializable;public class Message implements Serializable{		/**	 * 	 */	private static final long serialVersionUID = 1L;		private String uid;	private String from;	private String to;	private String groupId;			private Long eventTime;	private Integer commandId;			private Short type;	private Short status;	private Short direction;	private Short error;		private byte[] contents;			public Message() {		uid = java.util.UUID.randomUUID().toString();		from 	= "00000-00000-00000-00000-00000-000000";		to 		="00000-00000-00000-00000-00000-000000";		groupId = "00000-00000-00000-00000-00000-000000";		eventTime = System.currentTimeMillis();		commandId = MsgProtocol.Command.UNKNOWN;		type = MsgProtocol.MsgType.UNKNOWN;		status = MsgProtocol.MsgStatus.UNKNOWN;		error = MsgProtocol.Error.UNKNOWN;		direction = MsgProtocol.MsgDirection.UNKNOWN;		contents = "".getBytes();	}	public String getUid() {		return uid;	}	public void setUid(String uid) {		this.uid = uid;	}	public String getFrom() {		return from;	}	public void setFrom(String from) {		this.from = from;	}	public String getTo() {		return to;	}	public void setTo(String to) {		this.to = to;	}	public Long getEventTime() {		return eventTime;	}	public void setEventTime(Long eventTime) {		this.eventTime = eventTime;	}	public Integer getCommandId() {		return commandId;	}	public void setCommandId(Integer commandId) {		this.commandId = commandId;	}	public String getGroupId() {		return groupId;	}	public void setGroupId(String groupId) {		this.groupId = groupId;	}	public Short getType() {		return type;	}	public void setType(Short type) {		this.type = type;	}	public Short getStatus() {		return status;	}	public void setStatus(Short status) {		this.status = status;	}	public Short getDirection() {		return direction;	}	public void setDirection(Short direction) {		this.direction = direction;	}	public Short getError() {		return error;	}	public void setError(Short error) {		this.error = error;	}	public byte[] getContents() {		return contents;	}	public void setContents(byte[] contents) {		this.contents = contents;	}				}